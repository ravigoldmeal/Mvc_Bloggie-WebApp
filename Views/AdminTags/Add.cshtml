@model Bloggi.Models.ViewModels.AddTagRequest
@{
}

<div class="container py-5">
    <form method="post">

        <div class="mb-3">
           <label class="from-label">Name</label>
            <input type="text" class="form-control" id="name" asp-for ="Name" />
        </div> 
        <div class="mb-3">
           <label class="from-label">DisplayName</label>
            <input type="text" class="form-control" id="displayName" asp-for ="DisplayName" />
        </div>
        <div class="mb-3">
            <button type="submit" class="btn btn-dark">Submit </button>
        </div>
    </form>

</div>